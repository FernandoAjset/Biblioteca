//////////////////////////////////////////////////////////////////////////////////////////////////////////

        PARA JDK 11

//////////////////////////////////////////////////////////////////////////////////////////////////////////

PATH="/usr/lib/jvm/java-11-openjdk-amd64/bin"
JAVA_HOME="/usr/lib/jvm/java-11-openjdk-amd64/bin"

sudo update-alternatives --install "/usr/bin/java" "java" "/usr/lib/jvm/java-11-openjdk-amd64/bin/java" 0

sudo update-alternatives --install "/usr/bin/javac" "javac" "/usr/lib/jvm/java-11-openjdk-amd64/bin/javac" 0

sudo update-alternatives --set java /usr/lib/jvm/java-11-openjdk-amd64/bin/java
sudo update-alternatives --set javac /usr/lib/jvm/java-11-openjdk-amd64/bin/javac

//////////////////////////////////////////////////////////////////////////////////////////////////////////

        PARA JDK 13

//////////////////////////////////////////////////////////////////////////////////////////////////////////

PATH="/usr/lib/jvm/java-13.0.2/bin"
JAVA_HOME="/usr/lib/jvm/java-13.0.2/bin"

sudo update-alternatives --install "/usr/bin/java" "java" "/usr/lib/jvm/java-13.0.2/bin/java" 0

sudo update-alternatives --install "/usr/bin/javac" "javac" "/usr/lib/jvm/java-13.0.2/bin/javac" 0

sudo update-alternatives --set java /usr/lib/jvm/java-13.0.2/bin/java
sudo update-alternatives --set javac /usr/lib/jvm/java-13.0.2/bin/javac
